{
  "expo": {
    "name": "Mercately",
    "slug": "mercately",
    "version": "1.0.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mercately.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "android": {
      "versionCode": 7,
      "package": "com.mercately.app",
      "permissions": [
        "NOTIFICATIONS",
        "USER_FACING_NOTIFICATIONS",
        "AUDIO_RECORDING",
        "ACCESS_FINE_LOCATION",
        "INTERNET"
      ]
    },
    "sdkVersion": "38.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
